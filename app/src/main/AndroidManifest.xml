<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ford.zrouse.unfoundsound">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Login"
            android:label="@string/login_button_text" />
        <activity
            android:name=".SignUp"
            android:label="@string/sign_up_button_text" />
        <activity
            android:name=".Home"
            android:label="@string/home" />
        <activity android:name=".Search"
            android:label="@string/search"/>
        <activity android:name=".Playlists"
            android:label="@string/playlists"/>
        <activity android:name=".Trending"
            android:label="@string/trending"/>
    </application>

</manifest>